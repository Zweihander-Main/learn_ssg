{
	"python.languageServer": "None",
	"files.exclude": {
		".venv": true
	},
	"[python]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports.ruff": "always",
			"source.fixAll.ruff": "always"
		},
		"editor.defaultFormatter": "charliermarsh.ruff"
	},
	"editor.formatOnSave": true,
	"cSpell.words": [
		"htmlnode",
		"sublists",
		"textnode"
	],

}